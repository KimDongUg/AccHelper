{
  "headers": [
    {
      "source": "/(.*).html",
      "headers": [
        { "key": "Cache-Control", "value": "no-cache, no-store, must-revalidate" }
      ]
    },
    {
      "source": "/js/(.*)",
      "headers": [
        { "key": "Cache-Control", "value": "no-cache, no-store, must-revalidate" }
      ]
    },
    {
      "source": "/css/(.*)",
      "headers": [
        { "key": "Cache-Control", "value": "no-cache, no-store, must-revalidate" }
      ]
    }
  ],
  "rewrites": [
    { "source": "/api/:path*", "destination": "https://acchelperback.onrender.com/api/:path*" },
    { "source": "/admin.html", "destination": "/admin.html" },
    { "source": "/login.html", "destination": "/login.html" },
    { "source": "/register.html", "destination": "/register.html" },
    { "source": "/privacy.html", "destination": "/privacy.html" },
    { "source": "/terms.html", "destination": "/terms.html" },
    { "source": "/copyright.html", "destination": "/copyright.html" },
    { "source": "/contact.html", "destination": "/contact.html" }
  ]
}
